  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "fix-memory-limit": "cross-env LIMIT=2048 increase-memory-limit",
    "start": "set PORT=3030 && react-scripts start",
    "build": "react-scripts --max_old_space_size=4096 build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "electron-dev": "set PORT=3030 && concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3030 && electron .\"",
    "preelectron-pack": "react-scripts --max_old_space_size=4096 build",
    "electron-pack": "set ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true && build  — em.main=build/electron.js"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "cross-env": "^5.0.5",
    "electron": "^13.1.2",
    "electron-builder": "^20.38.5",
    "electron-rebuild": "^1.8.2",
    "increase-memory-limit": "^1.0.3",
    "source-map-explorer": "^1.6.0",
    "wait-on": "^2.1.2"
  },
  "build": {
    "appId": "au.coaas",
    "extends": null,
    "productName": "CoaaS IDE",
    "copyright": "Copyright © 2020 Alireza Hassani",
    "mac": {
      "category": "public.app-category.utilities",
      "icon": null
    },
    "win": {
      "target": "nsis",
      "icon": "build/logo.ico",
      "legalTrademarks": "CoaaS"
    },
    "files": [
      "build/**/*",
      "build/logo.*",
      "public/main.js"
    ],
    "extraResources": [],
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
